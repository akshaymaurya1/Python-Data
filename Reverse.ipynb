   "source": [
    "n=int(input(\"Please give a number :\"))\n",
    "print(\"Before revering number is :%d\"%n)\n",
    "reverse=0\n",
    "while n!=0:\n",
    "    reverse=reverse*10 + n%10\n",
    "    n=(n//10)\n",
    "print(\"After reverse :%d\"%reverse)"
   ]
